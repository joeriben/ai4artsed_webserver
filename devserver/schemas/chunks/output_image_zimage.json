{
  "name": "output_image_zimage",
  "type": "output_chunk",
  "backend_type": "comfyui",
  "media_type": "image",
  "requires_workflow": true,
  "description": "Z-Image - 6B parameter model with excellent text rendering (Chinese + English)",

  "workflow": {
    "6": {
      "inputs": {
        "text": "",
        "clip": ["11", 0]
      },
      "class_type": "CLIPTextEncode",
      "_meta": {
        "title": "CLIP Text Encode (Positive)"
      }
    },
    "8": {
      "inputs": {
        "samples": ["13", 0],
        "vae": ["10", 0]
      },
      "class_type": "VAEDecode",
      "_meta": {
        "title": "VAE Decode"
      }
    },
    "9": {
      "inputs": {
        "filename_prefix": "ai4artsed_zimage",
        "images": ["8", 0]
      },
      "class_type": "SaveImage",
      "_meta": {
        "title": "Save Image"
      }
    },
    "10": {
      "inputs": {
        "vae_name": "ae.safetensors"
      },
      "class_type": "VAELoader",
      "_meta": {
        "title": "Load VAE (Flux)"
      }
    },
    "11": {
      "inputs": {
        "clip_name": "qwen_3_4b.safetensors",
        "type": "z_image"
      },
      "class_type": "CLIPLoader",
      "_meta": {
        "title": "Load CLIP (Qwen 3 4B)"
      }
    },
    "12": {
      "inputs": {
        "unet_name": "z_image_bf16.safetensors"
      },
      "class_type": "UNETLoader",
      "_meta": {
        "title": "Load UNET (Z-Image)"
      }
    },
    "13": {
      "inputs": {
        "seed": 0,
        "steps": 30,
        "cfg": 4,
        "sampler_name": "euler",
        "scheduler": "normal",
        "denoise": 1,
        "model": ["12", 0],
        "positive": ["6", 0],
        "negative": ["14", 0],
        "latent_image": ["15", 0]
      },
      "class_type": "KSampler",
      "_meta": {
        "title": "KSampler"
      }
    },
    "14": {
      "inputs": {
        "text": "",
        "clip": ["11", 0]
      },
      "class_type": "CLIPTextEncode",
      "_meta": {
        "title": "CLIP Text Encode (Negative)"
      }
    },
    "15": {
      "inputs": {
        "width": 1024,
        "height": 1024,
        "batch_size": 1
      },
      "class_type": "EmptyLatentImage",
      "_meta": {
        "title": "Empty Latent Image"
      }
    },
    "26": {
      "inputs": {
        "value": ""
      },
      "class_type": "PrimitiveStringMultiline",
      "_meta": {
        "title": "ai4artsed_text_prompt"
      }
    }
  },

  "input_mappings": {
    "prompt": {
      "node_id": "6",
      "field": "inputs.text",
      "source": "{{PREVIOUS_OUTPUT}}",
      "description": "Main prompt - image description"
    },
    "negative_prompt": {
      "node_id": "14",
      "field": "inputs.text",
      "default": "",
      "description": "Negative prompt - what to avoid"
    },
    "width": {
      "node_id": "15",
      "field": "inputs.width",
      "default": 1024,
      "description": "Image width in pixels"
    },
    "height": {
      "node_id": "15",
      "field": "inputs.height",
      "default": 1024,
      "description": "Image height in pixels"
    },
    "steps": {
      "node_id": "13",
      "field": "inputs.steps",
      "default": 30,
      "description": "Number of sampling steps (30-50 recommended)"
    },
    "cfg": {
      "node_id": "13",
      "field": "inputs.cfg",
      "default": 4,
      "description": "Classifier Free Guidance scale"
    },
    "seed": {
      "node_id": "13",
      "field": "inputs.seed",
      "default": 0,
      "description": "Seed for reproducibility"
    }
  },

  "output_mapping": {
    "node_id": "9",
    "output_type": "image",
    "format": "png",
    "field": "filename_prefix",
    "description": "SaveImage node that outputs the generated image"
  },

  "meta": {
    "estimated_duration_seconds": "30-60",
    "quality_rating": 4,
    "requires_gpu": true,
    "gpu_vram_mb": 24000,
    "model_file": "z_image_bf16.safetensors",
    "text_encoder": "qwen_3_4b.safetensors",
    "vae_model": "ae.safetensors",
    "recommended_resolution": "1024x1024",
    "supported_resolutions": [
      "512x512",
      "768x768",
      "1024x1024",
      "1280x1280",
      "1024x768",
      "768x1024"
    ],
    "notes": "Z-Image 6B base model from Alibaba Tongyi Lab. Excellent at rendering text in images (Chinese + English). Better for LoRA training than Turbo.",
    "version": "1.0"
  }
}
