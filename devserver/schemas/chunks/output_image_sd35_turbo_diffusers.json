{
  "name": "output_image_sd35_turbo_diffusers",
  "type": "output_chunk",
  "backend_type": "diffusers",
  "media_type": "image",
  "description": "SD3.5 Large Turbo via HuggingFace Diffusers - 4-step distilled inference",

  "diffusers_config": {
    "model_id": "stabilityai/stable-diffusion-3.5-large-turbo",
    "pipeline_class": "StableDiffusion3Pipeline",
    "torch_dtype": "float16",
    "enable_attention_slicing": true,
    "enable_vae_tiling": false
  },

  "input_mappings": {
    "prompt": {
      "source": "{{PREVIOUS_OUTPUT}}",
      "description": "Positive prompt - main image description"
    },
    "negative_prompt": {
      "default": "",
      "description": "Negative prompt - not recommended for turbo models"
    },
    "width": {
      "default": 1024,
      "description": "Image width in pixels"
    },
    "height": {
      "default": 1024,
      "description": "Image height in pixels"
    },
    "steps": {
      "default": 4,
      "description": "Number of sampling steps (turbo: 4 optimal)"
    },
    "cfg": {
      "default": 0.0,
      "description": "CFG scale (turbo uses 0.0 - guidance embedded in distillation)"
    },
    "seed": {
      "default": "random",
      "description": "Seed for reproducibility"
    }
  },

  "output_mapping": {
    "output_type": "image",
    "format": "png",
    "description": "Generated PNG image"
  },

  "meta": {
    "estimated_duration_seconds": "3",
    "quality_rating": 2,
    "requires_gpu": true,
    "gpu_vram_mb": 8000,
    "model_file": "stabilityai/stable-diffusion-3.5-large-turbo (HuggingFace)",
    "recommended_resolution": "1024x1024",
    "supported_resolutions": [
      "512x512",
      "768x768",
      "1024x1024",
      "1280x1280"
    ],
    "fallback_chunk": "output_image_sd35_diffusers",
    "notes": "Adversarial Diffusion Distillation (ADD) model. 4 steps optimal, CFG 0.0 (guidance baked into distillation). Negative prompts have minimal effect.",
    "features": [
      "4-step generation: ~3s per image",
      "Live preview: See image evolve step-by-step",
      "Abort support: Cancel mid-generation",
      "Same encoder architecture as SD3.5 Large"
    ]
  }
}
