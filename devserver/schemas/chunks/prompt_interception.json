{
  "name": "prompt_interception",
  "description": "Universal Prompt Interception Template - Task+Context+Prompt Format",
  "template": "Task:\n{{TASK}}\n\nContext:\n{{CONTEXT}}\n\nPrompt:\n{{INPUT_TEXT}}",
  "backend_type": "ollama",
  "model": "gemma2:9b",
  "parameters": {
    "temperature": 0.7,
    "top_p": 0.9,
    "stream": false
  },
  "meta": {
    "chunk_type": "prompt_interception",
    "output_format": "text",
    "universal_interface": true,
    "supports_contexts": ["English", "Dadaism", "UK Youth Slang", "any"]
  }
}
