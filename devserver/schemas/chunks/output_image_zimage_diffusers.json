{
  "name": "output_image_zimage_diffusers",
  "type": "output_chunk",
  "backend_type": "diffusers",
  "media_type": "image",
  "description": "Z-Image Turbo 6B via HuggingFace Diffusers - Fast high-quality image generation with text rendering",

  "diffusers_config": {
    "model_id": "Tongyi-MAI/Z-Image-Turbo",
    "pipeline_class": "ZImagePipeline",
    "torch_dtype": "bfloat16",
    "enable_attention_slicing": true,
    "enable_vae_tiling": false
  },

  "input_mappings": {
    "prompt": {
      "source": "{{PREVIOUS_OUTPUT}}",
      "description": "Positive prompt - main image description"
    },
    "negative_prompt": {
      "default": "",
      "description": "Negative prompt - what to avoid"
    },
    "width": {
      "default": 1024,
      "description": "Image width in pixels"
    },
    "height": {
      "default": 1024,
      "description": "Image height in pixels"
    },
    "steps": {
      "default": 9,
      "description": "Number of sampling steps (Turbo: 8-9 recommended)"
    },
    "cfg": {
      "default": 0.0,
      "description": "Guidance scale (Turbo: 0.0 recommended, no CFG needed)"
    },
    "seed": {
      "default": "random",
      "description": "Seed for reproducibility"
    }
  },

  "output_mapping": {
    "output_type": "image",
    "format": "png",
    "description": "Generated PNG image"
  },

  "meta": {
    "estimated_duration_seconds": "15-30",
    "quality_rating": 5,
    "requires_gpu": true,
    "gpu_vram_mb": 16000,
    "model_file": "Tongyi-MAI/Z-Image-Turbo (HuggingFace)",
    "recommended_resolution": "1024x1024",
    "supported_resolutions": [
      "512x512",
      "768x768",
      "1024x1024",
      "1280x1280"
    ],
    "notes": "Z-Image Turbo 6B from Alibaba Tongyi Lab. 8-step distilled model with excellent text rendering. Supports Chinese & English."
  }
}
